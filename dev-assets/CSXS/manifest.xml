<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest 
    Version="6.0" 
    ExtensionBundleId="com.integnity.vuepanels.dev" 
    ExtensionBundleVersion="1.0.0"
    ExtensionBundleName="Integnity - Vue Panels (dev)" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <ExtensionList>
    <Extension Id="com.integnity.vuepanels.dev.panel" Version="1.0" />
  </ExtensionList>

  <ExecutionEnvironment>
    <HostList>
        <Host Name="PPRO" Version="9.0"/>
        <Host Name="AEFT" Version="13.0"/>
    </HostList>

    <LocaleList>
      <Locale Code="All" />
    </LocaleList>

    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="6.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>

  <DispatchInfoList>
    <Extension Id="com.integnity.vuepanels.dev.panel">
      <DispatchInfo >
    
        <Resources>
          <MainPath>./index.html</MainPath>
          <ScriptPath>./loader.jsx</ScriptPath>

          <CEFCommandLine>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>   

        </Resources>
      
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
      
        <UI>

          <Type>Panel</Type>
          <Menu>Integnity - Vue Panels (dev)</Menu>

          <Geometry>
            <Size>
              <Height>1200</Height>
              <Width>840</Width>
            </Size>
            <MinSize>
              <Height>100</Height>
              <Width>150</Width>
            </MinSize>
            <MaxSize>
              <Height>1200</Height>
              <Width>840</Width>
            </MaxSize>
          </Geometry>

          <Icons>
            <Icon Type="Normal">./icons/iconNormal.png</Icon>
            <Icon Type="RollOver">./icons/iconRollover.png</Icon>
            <Icon Type="Disabled">./icons/iconDisabled.png</Icon>
            <Icon Type="DarkNormal">./icons/iconDarkNormal.png</Icon>
            <Icon Type="DarkRollOver">./icons/iconDarkRollover.png</Icon>
          </Icons>  
                  
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>